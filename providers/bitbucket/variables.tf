variable "username" {
  type = string
  description = "The Bitbucket username"
  default     = "default"
}
variable "password" {
  type = string
  description = "The Bitbucket password"
  default     = "default"
}
variable "oauth_client_id" {
  type = string
  description = "The Bitbucket oauth_client_id"
  default     = "default"
}
variable "oauth_client_secret" {
  type = string
  description = "The Bitbucket oauth_client_secret"
  default     = "default"
}
variable "oauth_token" {
  type = string
  description = "The Bitbucket oauth_token"
  default     = "default"
}
